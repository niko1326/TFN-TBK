{
    "name": "gamedex-v2",
    "version": "1.0.0",
    "private": true,
    "description": "GameDex v2 - modular React + Webpack + GitHub Pages (subfolder TFN/z3)",
    "homepage": "https://niko1326.github.io/TFN-TBK/TFN/z3/",
    "scripts": {
      "start": "cross-env NODE_ENV=development PUBLIC_PATH=/ webpack serve",
      "build": "cross-env NODE_ENV=production PUBLIC_PATH=/REPO_NAME/TFN/z3/ webpack --progress",
      "postbuild:docs": "rm -rf ../../docs/TFN/z3 && mkdir -p ../../docs/TFN/z3 && cp -r dist/* ../../docs/TFN/z3/",
      "build:docs": "npm run build && npm run postbuild:docs"
    },
    "dependencies": {
      "react": "^18.3.1",
      "react-dom": "^18.3.1"
    },
    "devDependencies": {
      "@babel/core": "^7.25.2",
      "@babel/preset-env": "^7.25.4",
      "@babel/preset-react": "^7.24.7",
      "babel-loader": "^9.1.3",
      "clean-webpack-plugin": "^4.0.0",
      "css-loader": "^7.1.2",
      "cross-env": "^7.0.3",
      "html-loader": "^5.0.0",
      "html-webpack-plugin": "^5.6.0",
      "style-loader": "^4.0.0",
      "webpack": "^5.96.1",
      "webpack-cli": "^5.1.4",
      "webpack-dev-server": "^5.0.4"
    }
  }